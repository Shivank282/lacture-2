#include <iostream>
using namespace std;

int main() {
    int n = 5;

    for(int i = n; i >= 1; i--) {
for(int space = 1; space <= 5*(n - i); space++) {
            cout << " ";
        }
for(int star = 1; star <= i; star++) {
            cout << "* ";
        }

        cout << endl;
    }

    return 0;
}
